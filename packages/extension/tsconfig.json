{
  "extends": "../../tsconfigs/common/tsconfig.module.browser.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "components/*": [
        "./src/components/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    // "jsxImportSource": "react",
    "types": [
      "vite/client",
      "vite-plugin-solid-svg/types",
      "chrome"
    ],
  },
  "include": [
    "src",
  ]
}